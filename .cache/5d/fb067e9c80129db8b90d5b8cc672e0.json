e;\n  font-family: \"Montserrat\", sans-serif;\n  overflow-x: hidden;\n}\n:root {\n  --body-bg-color: rgb(45, 46, 46);\n  --secondary-bg-color: gold;\n}\n\n/* <---- Header Styles ----> */\nnav {\n  min-height: 10vh;\n  max-height: 10vh;\n  width: 100%;\n  padding: 0.5rem 0;\n\n  /* background: rgb(236, 245, 113); */\n  background: rgb(45, 46, 46);\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  align-items: center;\n  color: white;\n  z-index: 0;\n}\nheader #title-header {\n  position: absoulte;\n  margin-left: 3.5rem;\n  margin-top: -1.5rem;\n  font-size: 3rem;\n  background: -webkit-linear-gradient(#a867be, #6c63ff);\n  -webkit-background-clip: text;\n  background-clip: text;\n  -webkit-text-fill-color: transparent;\n}\nnav .nav-dummy {\n  background: var(--body-bg-color);\n  width: 10vw;\n  min-height: 10vw;\n  flex: 1 1 32rem;\n}\nnav .nav-links {\n  display: flex;\n  margin-right: 8rem;\n  margin-top: -5rem;\n  flex: 3 1 40rem;\n  z-index: 2;\n}\nnav .nav-links li {\n  margin-right: 2.6rem;\n  list-style: none;\n}\nnav .nav-links a {\n  padding: 0.1rem 0.5rem;\n  text-decoration: none;\n  font-size: 1.4rem;\n  outline-color: black;\n  background: -webkit-linear-gradient(#a867be, #6c63ff);\n  -webkit-background-clip: text;\n  background-clip: text;\n  -webkit-text-fill-color: transparent;\n}\nnav .nav-links li {\n  position: relative;\n}\n.active .navlinks a {\n  color: gray;\n}\n.sub-menu1 {\n  display: none;\n}\n.nav-links li:hover .sub-menu1 {\n  position: absolute;\n  display: block;\n  margin-left: 1rem;\n  z-index: 100;\n}\n.nav-links li:hover .sub-menu1 ul {\n  position: relative;\n  display: block;\n  margin: 10px;\n  background: rgb(42, 43, 43);\n  /* background: rgb(68, 68, 68); */\n  opacity: 100%;\n  border: 1px solid rgb(32, 32, 32);\n  width: 22rem;\n  padding: 0.5rem 0;\n}\n.nav-links li:hover .sub-menu1 ul li {\n  position: relative;\n  padding: 0.25rem;\n  display: block;\n  margin: 10px;\n}\n\n/* <---- Footer Styles ----> */\nfooter {\n  min-height: 10vh;\n  margin-top: 2rem;\n  display: flex;\n  align-items: center;\n  flex-wrap: wrap;\n}\nfooter h4 {\n  flex: 3 1 40rem;\n  font-size: calc(1.1rem + 1vw);\n  background: -webkit-linear-gradient(#a867be, #6c63ff);\n  -webkit-background-clip: text;\n  background-clip: text;\n  -webkit-text-fill-color: transparent;\n  margin-left: 3rem;\n}\nfooter ul {\n  display: flex;\n  flex: 2 1 30rem;\n  justify-content: space-between;\n  align-items: center;\n  margin-right: 3rem;\n}\nfooter a {\n  outline-color: black;\n}\n\n/* <---- Media Queries ----> */\n@media screen and (max-width: 1292px) {\n  nav {\n    justify-content: flex-end;\n  }\n  .nav-links {\n    flex: none;\n    position: relative;\n    right: -30rem;\n    top: -2.5rem;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"e5f256afd78a0a310b7eb78214a80d8d","cacheData":{"env":{}}}