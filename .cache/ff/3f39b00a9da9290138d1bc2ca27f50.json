nsparent 50%, rgb(71, 126, 172) 50%),\n    linear-gradient(135deg, rgb(71, 126, 172) 50%, transparent 50%),\n    linear-gradient(to right, skyblue, skyblue);\n  border: 1px solid rgb(77, 114, 170);\n  outline-color: black;\n}\n.controls .start-notes-container select {\n  width: 4.5rem;\n  height: 1.5rem;\n}\n.controls .octave-container select {\n  width: 3.5rem;\n  height: 1.5rem;\n}\n\n.chord-notes {\n  margin-bottom: 1rem;\n  margin-left: 1.7rem;\n  min-height: 15vh;\n  display: flex;\n  flex-direction: column;\n  flex-wrap: nowrap;\n  justify-content: flex-start;\n  align-items: center;\n  align-self: flex-start;\n}\n.chord-notes p {\n  font-size: 1.7rem;\n  margin-top: 0.5rem;\n  text-align: start;\n  align-self: flex-start;\n  background: -webkit-linear-gradient(#a867be, #6c63ff);\n  -webkit-background-clip: text;\n  background-clip: text;\n  -webkit-text-fill-color: transparent;\n}\n#intervals {\n  margin-left: 0.1rem;\n}\n\n/* ----- KEYBOARD STYLES ----- */\n.sticky {\n  position: fixed;\n  top: 0;\n  width: 98%;\n  background: rgb(45, 46, 46);\n  z-index: 100;\n  margin: 0;\n  padding-left: 1.7rem;\n  padding-top: 1rem;\n}\n.keyboard-container {\n  width: 60%;\n  height: 25vh;\n  min-height: 10rem;\n  position: absolute;\n  right: 0;\n  overflow-y: hidden;\n  overflow-x: scroll;\n  white-space: nowrap;\n}\n.keyboard {\n  position: relative;\n  border: 3px solid black;\n  min-width: 151.8%;\n  height: 102%;\n}\n.keyboard:before {\n  content: \" \";\n  position: absolute;\n  z-index: 0;\n  top: -2px;\n  left: -2px;\n  right: -1px;\n  bottom: -2px;\n  border: 2px solid #ffffff;\n}\n.note {\n  display: inline-block;\n  float: none;\n  zoom: 1;\n}\n\n.white-note {\n  position: relative;\n  width: 1.8rem;\n  min-height: 100%;\n  background: white;\n  border: 0.5px solid rgb(75, 75, 75);\n}\n.black-note {\n  position: relative;\n  background: black;\n  width: 1.1rem;\n  min-height: 60%;\n  vertical-align: top;\n  margin-left: -0.55rem;\n  margin-right: -0.55rem;\n  z-index: 1;\n}\n.bn-margin {\n  margin-left: 0.5rem;\n}\n.white-key-pressed {\n  background: rgb(190, 190, 190);\n}\n.black-key-pressed {\n  background: rgb(71, 71, 71);\n}\n.white-key-normal {\n  background: white;\n  border: 1px solid white;\n}\n.black-key-normal {\n  background: black;\n}\n"},"lineCount":null}},"error":null,"hash":"01833c08ee4af2f256b08ac39da7af81","cacheData":{"env":{}}}